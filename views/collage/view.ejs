    	<script>
        var socket = null;
        function connectWS() {
            if (socket == null) {
                socket = io.connect();
                socket.on("connect", function() {
                    socket.emit("joinRoom",{collage:"<%=collage._id%>"});
                });
                socket.on('message', function(data) {});
                socket.on('disconnect', function() {});
            }
            socket.socket.connect();
        }
		$(function(){
			connectWS();
		});
		</script>
